# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# PostgreSQL Database URL
DATABASE_URL=postgresql://bot_user:your_password@localhost:5432/kufar_bot

# Yandex Maps API Key (get from https://developer.tech.yandex.ru/)
YANDEX_MAPS_API_KEY=your_yandex_api_key_here

# Optional
NODE_ENV=production
LOG_LEVEL=info

# Parse interval in seconds (default: 300 = 5 minutes)
# For testing, you can set it to 10 seconds
PARSE_INTERVAL_SECONDS=300
